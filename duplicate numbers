def find_duplicates(nums):
    seen = set()
    duplicates = set()
    
    for num in nums:
        if num in seen:
            duplicates.add(num)
        else:
            seen.add(num)
    
    return list(duplicates)

# Example usage:
numbers = [1, 2, 3, 4, 3, 2, 5, 6, 1]
print("Duplicate numbers:", find_duplicates(numbers))
