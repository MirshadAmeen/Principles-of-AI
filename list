# Creating a list
my_list = [3, 1, 4, 1, 5, 9, 2, 6, 5, 3]

# Accessing elements
print("Elements in the list:")
for item in my_list:
    print(item)

# Accessing elements by index
print("\nElement at index 0:", my_list[0])
print("Element at index 4:", my_list[4])

# Slicing a list
print("\nSliced list from index 2 to 5:", my_list[2:6])

# Modifying elements
my_list[1] = 10
print("\nList after modifying element at index 1:", my_list)

# Appending elements
my_list.append(8)
print("\nList after appending 8:", my_list)

# Removing elements
my_list.remove(5)
print("\nList after removing first occurrence of 5:", my_list)

# Sorting elements
my_list.sort()
print("\nSorted list:", my_list)

# Reversing elements
my_list.reverse()
print("\nReversed list:", my_list)

# Finding length of the list
print("\nLength of the list:", len(my_list))

# Checking membership
print("\nIs 10 present in the list?", 10 in my_list)
print("Is 7 present in the list?", 7 in my_list)

# Clearing the list
my_list.clear()
print("\nList after clearing all elements:", my_list)
